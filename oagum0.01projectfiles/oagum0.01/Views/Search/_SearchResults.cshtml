@model oagum0._01.Models.T_Article



<tr>
    <td>
        <a class ="articleLink" id="displayPdf" href="@Model.Source">
            @Model.Title
        </a>
    </td>
    <td>
        @Html.ActionLink("Details", "Details", new { id = Model.ID })
    </td>
    <td>
        <button id="favorites" data-id ="@Model.ID" onclick="AddUserFavorite(this,@Model.ID)">
            <img src="~/Images/star.png" />
            
        </button>
    </td>
</tr>
<p style="display: none; position: absolute; left: 200px; top: 20px">
    <iframe style="width: 618px; height: 800px" id="myFrame" src=""></iframe>
</p>
